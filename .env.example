# Banco de dados MySQL
DATABASE_URL=mysql://user:password@localhost:3306/iphone_store

# JWT Secret para autenticação
JWT_SECRET=sua_chave_secreta_muito_segura_aqui

# Ambiente
NODE_ENV=production
PORT=3000

# OAuth (se usar autenticação Manus)
VITE_APP_ID=seu_app_id
OAUTH_SERVER_URL=https://api.manus.im
VITE_OAUTH_PORTAL_URL=https://portal.manus.im
OWNER_NAME=seu_nome
OWNER_OPEN_ID=seu_open_id

# Duttyfy (para pagamentos PIX)
DUTTYFY_API_KEY=sua_chave_api_duttyfy
DUTTYFY_API_URL=https://api.duttyfy.com

# UTMify (para rastreamento de conversões)
UTMIFY_API_KEY=sua_chave_utmify
UTMIFY_WORKSPACE_ID=seu_workspace_id

# Manus APIs (se usar)
BUILT_IN_FORGE_API_KEY=sua_chave_forge
BUILT_IN_FORGE_API_URL=https://api.manus.im

# Analytics (opcional)
VITE_ANALYTICS_ENDPOINT=seu_endpoint_analytics
VITE_ANALYTICS_WEBSITE_ID=seu_website_id
